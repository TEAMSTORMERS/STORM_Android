<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/roundstart_member"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/h_roundstart_member1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/participant_name"/>

    <TextView
        android:id="@+id/participant_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="이영진"
        android:fontFamily="@font/notosans_medium"
        android:includeFontPadding="false"
        android:textColor="@color/storm_gray"
        app:layout_constraintStart_toEndOf="@+id/roundstart_member"
        app:layout_constraintTop_toTopOf="@+id/roundstart_member"
        app:layout_constraintBottom_toBottomOf="@+id/roundstart_member"/>

</androidx.constraintlayout.widget.ConstraintLayout>